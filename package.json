    "babel-runtime": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-babel": "^6.0.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-watch": "^2.0.6",
    "catw": "^1.0.1",
    "chokidar-cli": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "run-sequence": "^1.2.2"
